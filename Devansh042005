#include <stdio.h>

int main(){
   char ch;
   printf("enter the character : ");
   scaf("%c" , &ch);
   
   if(ch >= 'A' && ch <= 'Z'){
      printf ("upper case");
      
   }
   else if (ch >= 'a' && ch <= 'z'){
      printf("lower case");
   }
   else{
      printf("not english letter");
   }
   return 0;
}
